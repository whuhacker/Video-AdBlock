{
    "name": "AdBlock for Youku Videos",
    "version": "0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "20.0",
    "description": "Just An Experimental Extension",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "http://valf.atm.youku.com/*"
    ],
    "background": {
        "scripts": [
            "redirect.js"
        ]
    },
    "web_accessible_resources": [
        "offline/empty.json"
    ]
}
